				if (comboBox_2.getSelectedIndex() != 0) {
					ControllerAuxiliar.preencherCamposGrupo(comboBox_2, tfNomeCadastrarProduto,
							tfCodigoCadastrarProduto, tfMedidaCadastrarProduto, cbUnidadeCadastrarProduto);
				} else {
					ControllerValidationProduto.resetarCamposGrupoProduto(tfNomeCadastrarProduto,
							tfCodigoCadastrarProduto, tfMedidaCadastrarProduto, cbUnidadeCadastrarProduto);
				}
			}
		});

