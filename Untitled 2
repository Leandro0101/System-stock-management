						dcVencimento_1, comboBox_2);
			}
		});

		bt_limpar_af_2.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent arg0) {
				ControllerValidationProduto.resetarTodosOsCampos(formattedTextField_1_1, spinner_1, editorPane_1,
						dcFabricacao_1, dcVencimento_1, comboBox_2);
			}
		});

		// ------------------ActionListeners de Atualizar o Produto
