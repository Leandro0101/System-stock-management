				if (comboBox.getSelectedIndex() != 0) {
					ControllerAuxiliar.preencherCamposGrupo(comboBox, textField, formattedTextField, textField_2,
							comboBox_1);
				} else {
					ControllerValidationProduto.resetarCamposGrupoProduto(textField, formattedTextField, textField_2,
